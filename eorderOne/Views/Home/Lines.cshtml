@model List<eorderOne.Models.ViewModels.listMlineaViewModel>

<div class="container">
    <div class="row">
        <div class="col-sm-12">
            <table class="table table-dark">
                <thead>
                    <tr>
                        <th scope="col">Empresa</th>
                        <th scope="col">Local</th>
                        <th scope="col">Nombre</th>
                        <th scope="col">Acciones</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var i in Model)
                    {
                        <tr>
                            <td>@i.ID_EMPRESA</td>
                            <td>@i.ID_LOCAL</td>
                            <td>@i.linea</td>
                            <td>

                                <a class="btn btn-secondary" href="~/Home/deleteLine/@i.linea">Borrar</a>
                            </td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
</div>